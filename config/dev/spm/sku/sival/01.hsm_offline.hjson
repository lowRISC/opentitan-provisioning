// The following operations are for testing purposes only. They are not
// intended for production use.
//
// $ source config/dev/env/spm.env
// $ cd /var/lib/opentitan/spm/sku/sival
// $ bin/hsmtool exec 01.hsm_offline_keygen.hjson
[
    // Import public keys.
    {
        "command": "rsa-import",
        "label": "spm-rsa-wrap-v0",
        "public": true,
        "public_attrs": {
            CKA_ENCRYPT: true,
            CKA_VERIFY: true,
            CKA_WRAP: true,
            CKA_TOKEN: true,
        },
        "filename": "../spm-rsa-wrap-v0.pem"
    }

    // Key generation commands.
    {
        "command": "aes-generate",
        label: "sival-aes-wrap-v0",
        "template": {
            CKA_ENCRYPT: true,
            CKA_DECRYPT: true,
            CKA_WRAP: true,
            CKA_UNWRAP: true,
            CKA_SENSITIVE: true,
            CKA_EXTRACTABLE: true,
            CKA_TOKEN: true,
        },
    }
    {
        "command": "rsa-generate",
        "label": "sku-sival-rsa-rma-v0",
        "key_length": 3072,
        "public_exponent": 65537,
        "wrapping": true,
        "extractable": false,
        "public_template": {
            CKA_CLASS: "CKO_PUBLIC_KEY",
            CKA_LABEL: "sku-sival-rsa-rma-v0.pub",
            CKA_ENCRYPT: true,
            CKA_VERIFY: true,
            CKA_TOKEN: true,
        },
        "private_template": {
            CKA_CLASS: "CKO_PRIVATE_KEY",
            CKA_LABEL: "sku-sival-rsa-rma-v0.priv",
            CKA_DECRYPT: true,
            CKA_SIGN: true,
            CKA_TOKEN: true,
            CKA_EXTRACTABLE: true,
        }
    }
    {
        "command": "ecdsa-generate",
        "label": "sival-dice-key-p256-v0"
        "curve": "1.2.840.10045.3.1.7",
        "wrapping": false,
        "extractable": false,
        "public_template": {
            CKA_LABEL: "sival-dice-key-p256-v0.pub",
            CKA_VERIFY: true,
            CKA_TOKEN: true,
        },
        "private_template": {
            CKA_LABEL: "sival-dice-key-p256-v0.priv",
            CKA_SIGN: true,
            CKA_TOKEN: true,
            CKA_EXTRACTABLE: true,
            CKA_SENSITIVE: true,
        }
    }
    {
        "command": "kdf-generate",
        "label": "sival-kdf-hisec-v0",
        "template": {
            CKA_DERIVE: true,
            CKA_SENSITIVE: true,
            CKA_EXTRACTABLE: true,
            CKA_TOKEN: true,
        }
    }
    {
        "command": "kdf-generate",
        "label": "sival-kdf-losec-v0",
        "template": {
            CKA_DERIVE: true,
            CKA_SENSITIVE: true,
            CKA_EXTRACTABLE: true,
            CKA_TOKEN: true,
        }
    }

    // Key export commands.
    {
        "command": "aes-export",
        "label": "sival-aes-wrap-v0",
        "wrap": "spm-rsa-wrap-v0",
        "wrap_mechanism": "RsaPkcs",
        "output": "sival-aes-wrap-v0.bin"
    }
    {
        "command": "rsa-export",
        "label": "sku-sival-rsa-rma-v0.pub",
        "private": false,
        "format": "Pem",
        "filename": "sku-sival-rsa-rma-v0.pub.pem"
    }
    {
        "command": "ecdsa-export",
        "label": "sival-dice-key-p256-v0.pub",
        "private": false,
        "format": "Pem",
        "filename": "sival-dice-key-p256-v0.pub.pem"
    }
    {
        "command": "ecdsa-export",
        "label": "sival-dice-key-p256-v0.priv",
        "private": true,
        "format": "Pem",
        "wrap": "sival-aes-wrap-v0",
        "filename": "sival-dice-key-p256-v0.pem"
    }
    {
        "command": "kdf-export",
        "label": "sival-kdf-hisec-v0",
        "wrap": "sival-aes-wrap-v0",
        "wrap_mechanism": "AesKeyWrap",
        "output": "sival-kdf-hisec-v0.bin"
    }
    {
        "command": "kdf-export",
        "label": "sival-kdf-losec-v0",
        "wrap": "sival-aes-wrap-v0",
        "wrap_mechanism": "AesKeyWrap",
        "output": "sival-kdf-losec-v0.bin"
    }
]

